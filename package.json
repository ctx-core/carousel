{
	"name": "@ctx-core/carousel",
	"version": "9.0.12",
	"description": "ctx-core carousel",
	"keywords": [
		"ctx-core",
		"carousel"
	],
	"homepage": "https://github.com/ctx-core/carousel#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/carousel/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/carousel.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./src/index.js",
	"module": "./src/index.js",
	"types": "./src/index.d.ts",
	"scripts": {
		"build": "npm run clean && npm run compile && npm run symlink",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f dist/**/*.js && rm -f dist/**/*.d.ts && rm -f dist/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"symlink": "./node_modules/.bin/symlink_src_assets_to_dist .",
		"exec": "$@"
	},
	"devDependencies": {
		"@ctx-core/ctx-core-package-tools": "workspace:^8.0.0",
		"@ctx-core/fontawesome": "workspace:^14.0.0",
		"@ctx-core/html": "workspace:^10.0.0",
		"@ctx-core/object": "workspace:^20.0.0",
		"svelte": "3.38.2",
		"typescript": "^4.3.2"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
